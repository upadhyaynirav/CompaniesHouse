        def varName = key.replace(&apos;.Threads&apos;, &apos;&apos;).replace(&apos;-&apos;, &apos;_&apos;).toUpperCase() + &apos;_THREADS&apos;
        vars.put(varName, value)
        log.info(&quot;Loaded: ${varName} = ${value}&quot;)
    }
