              <stringProp name="parameters"></stringProp>
              <stringProp name="script">// Read all booking IDs from CSV
def lines = new File(&quot;booking_ids.csv&quot;).readLines().findAll { it.trim() }

if (lines.isEmpty()) {
    log.error(&quot;No booking IDs found in CSV&quot;)
    vars.put(&quot;bookingId&quot;, &quot;ERROR_NO_IDS&quot;)
} else {
    // Pick random ID
    Random random = new Random()
    def randomId = lines[random.nextInt(lines.size())].trim()
    vars.put(&quot;bookingId&quot;, randomId)
    //log.info(&quot;Selected random booking ID: &quot; + randomId)
}</stringProp>
