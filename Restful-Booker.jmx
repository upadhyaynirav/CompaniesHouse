            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Write Booking Ids to File" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">
int NoOfBookingIds=Integer.valueOf((null==vars.get(&quot;bookingIds_matchNr&quot;)?0:vars.get(&quot;bookingIds_matchNr&quot;)));

//log.info(&quot;********* No of BookngIds&quot; + NoOfBookingIds);

if (NoOfBookingIds == 0) {
    log.warn(&quot;⚠️ *** No IDs extracted, file will not be written.&quot;)
} else {
    def file = new File(&quot;booking_ids.csv&quot;)
    file.text = &quot;&quot;  // clear file before writing
    
    // Loop through each extracted ID
    for (int i = 1; i &lt;= NoOfBookingIds; i++) {
        String bookingId = vars.get(&quot;bookingIds_&quot; + i)
        file &lt;&lt; bookingId + System.lineSeparator()

    }
    file.text = file.text.trim()
}
</stringProp>
